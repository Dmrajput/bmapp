{
  "expo": {
    "name": "ReelSound",
    "slug": "BMApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/reelsound.png",
    "scheme": "reelsound",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.divyaraj_padhiyar.ReelSound",
      "permissions": ["INTERNET", "READ_MEDIA_AUDIO"]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2136043836079463~9890171450",
          "iosAppId": "ca-app-pub-xxxxxxxx~zzzzzzzz"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7604842e-650e-4ac0-8a83-cde4fc533ed9"
      },
      "API_URL": "https://bmapp-ypzi.onrender.com"
    },
    "owner": "divyaraj_padhiyar"
  }
}
